# Server Configuration
VITE_PORT=5000
VITE_NODE_ENV=development

# Client URL (for CORS)
VITE_CLIENT_URL=http://localhost:3000

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_API_KEY=your_api_key
VITE_CLOUDINARY_API_SECRET=your_api_secret

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your_publishable_key

# Video Chat Configuration (Twilio)
VITE_TWILIO_ACCOUNT_SID=your_account_sid
VITE_TWILIO_API_KEY=your_api_key

# Feature Flags
VITE_ENABLE_VIDEO_PROCESSING=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_SOCIAL_FEATURES=true
VITE_ENABLE_ACHIEVEMENTS=true
VITE_ENABLE_NUTRITION_TRACKING=true
VITE_ENABLE_WORKOUT_TRACKING=true
VITE_ENABLE_PROGRESS_TRACKING=true

# Note: The following variables should be in a backend .env file
# and not exposed to the frontend:
# - DB_HOST
# - DB_PORT
# - DB_NAME
# - DB_USER
# - DB_PASSWORD
# - JWT_SECRET
# - JWT_EXPIRE
# - CLOUDINARY_API_SECRET
# - STRIPE_SECRET_KEY
# - STRIPE_WEBHOOK_SECRET
# - TWILIO_AUTH_TOKEN
# - TWILIO_API_SECRET
# - EMAIL_FROM
# - EMAIL_PASSWORD
# - REDIS_HOST
# - REDIS_PORT
# - REDIS_PASSWORD
# - RATE_LIMIT_WINDOW
# - RATE_LIMIT_MAX
# - LOG_LEVEL
# - LOG_FILE_PATH
